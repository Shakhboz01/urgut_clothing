<table class="table table-hover table-sm table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Цена</th>
      <th>пополнение oт</th>
      <th>Оформлено в</th>
      <th>Исполнитель</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% transaction_histories.each do |transaction_history| %>
      <tr>
        <td><%= transaction_history.id %></td>
        <td><%= currency_convert(transaction_history.price_in_usd, transaction_history.price) %></td>
        <td><%= check_belonging(transaction_history) %></td>
        <td><%= strf_datetime transaction_history.created_at %></td>
        <td><%= transaction_history.user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
